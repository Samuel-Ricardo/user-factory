{
  "name": "user-factory",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "nodemon --watch ./src --exec ts-node -r tsconfig-paths/register ./src/server.ts -e ts",
    "docker:startup": "npm run test && npm run start:dev",
    "build": "tsc && tsc-alias",
    "husky:prepare": "husky install",
    "lint": "eslint ./src/** --fix --no-error-on-unmatched-pattern",
    "pretty": "prettier --no-error-on-unmatched-pattern --write --check ./src/**",
    "coverage": "jest --coverage --passWithNoTests",
    "test": "jest --passWithNoTests --color",
    "test:dev": "jest --watchAll --color",
    "test:staged": "jest --findRelatedTests  --passWithNoTests --color .",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prisma": "^4.13.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.0",
    "prettier": "^2.8.8"
  }
}
